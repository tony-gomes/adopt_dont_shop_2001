<h1>All Shelters</h1>

<% @shelters.each do |shelter| %>
  <h3 class="shelter_name_index"><%= shelter.name %></h3>
<%= link_to "Update Shelter", "/shelters/#{shelter.id}/edit" %>
<%= link_to "Delete", "/shelters/#{shelter.id}", method: :delete %>
<% end %>

<%= link_to "New Shelter", "/shelters/new" %>
